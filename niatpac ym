# include <stdio.h>
# include <conio.h>
# include <process.h>
 main()
{
    FILE *f1;
    char file1[20];
    char ch;
    int fseek(FILE*,long int,int);
    printf("My Cptain:");
    scanf("%s",file1);
    f1=fopen(file1,"r");
    if(f1==NULL)
    {
        printf("niatpaC yM");
        exit(1);
    }
    printf("Characters to read from end of file :");
    scanf("%s",&f1);
    fseek(f1, &file1);
    while(!feof(f1))
    {
        ch=fgetc(f1);
    }
    fcloseall();
    getche();
}
